class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        notend = True
        i = 0
        j = 1
        while notend:
            
          
            num1 = int(nums[i])
            num2 = int(nums[j])
            if num1 + num2 == target:
                notend = False
            else: 
                j= j+1

                if j >= len(nums):
                   
                    i = i + 1
                    j = i + 1
    

        return i,j
